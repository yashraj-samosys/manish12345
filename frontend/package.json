{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3087",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "http-server dist/frontend -p 3087",
    "build-aot": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --aot --build-optimizer --configuration=production && gzipper compress ./dist",
    "build-aot-hashing": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --aot --output-hashing=all --build-optimizer --configuration=production && gzipper compress ./dist"
  },
